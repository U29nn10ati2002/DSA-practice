#include <bits/stdc++.h>
using namespace std;
vector<int>arr;
auto main()->int{
    int size;
    cin >> size;
    arr.resize(100,0);
    for (int i = 0; i != size; i++){
        int n;
        cin >> n;
        arr[n] = arr[n] + 1;
    }
    for (int r = 0; r != arr.size(); r++){
        cout << arr[r]<<" ";
    }
    return 0;
}
