#include<bits/stdc++.h>
using namespace std;

bool isTargetPresentIn2Dvector(vector<vector<int> > A, int target){
    for(int i=0; i<A.size(); i++){
        for(int j=0; j<A[i].size(); j++){
            if(A[i][j]==target){
                return true;
            }
        }
    }
    return false;
}

int main(){
    vector<vector<int> >A = {{2,8,9},
                            {3,4,5}};
    int target = 8;
    isTargetPresentIn2Dvector(A, target);
}
